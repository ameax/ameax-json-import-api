{
    "name": "ameax/ameax-json-import-api",
    "description": "A PHP package for sending data to Ameax via their JSON API",
    "keywords": [
        "ameax",
        "api",
        "json",
        "php",
        "ameax-json-import-api"
    ],
    "homepage": "https://github.com/ameax/ameax-json-import-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Schmidt",
            "email": "69188126+ms-aranes@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ameax\\AmeaxJsonImportApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ameax\\AmeaxJsonImportApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "version": "1.0.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
